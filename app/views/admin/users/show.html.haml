
.popular-activities-container
  .popular-activities-inner-container
    .populartext
      %h1  Modify users
      %table
        %tbody
          - @users.each do |user|
            %tr
              %td= user.forename
              %td= user.surname
              %td= user.email
              %td= user.id
              %td
                = link_to 'Edit', edit_admin_user_path(user.id), class:'btn btn-default'
                = link_to 'Delete', admin_user_path(user.id), method: :delete, class:'btn btn-danger'








